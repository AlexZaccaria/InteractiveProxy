{
  "name": "interactive-proxy",
  "version": "1.0.0",
  "description": "Interactive Proxy - Development proxy server with modern UI for HTTP/HTTPS/WebSocket interception, local resource management, and request analysis",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "client": "npm --prefix client start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm --prefix client run build",
    "install-client": "npm --prefix client install",
    "install-all": "npm install && npm run install-client"
  },
  "keywords": [
    "proxy",
    "http",
    "https",
    "websocket",
    "mitm",
    "interceptor",
    "debugging",
    "development",
    "mock",
    "testing",
    "request-analysis",
    "local-resources"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/interactive-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/interactive-proxy/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/interactive-proxy#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fzstd": "^0.1.1",
    "http-proxy-middleware": "^2.0.6",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.1",
    "pako": "^2.1.0",
    "ws": "^8.14.2",
    "zstd-codec": "^0.1.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
